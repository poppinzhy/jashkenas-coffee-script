#!/usr/bin/env node

process.mixin(require('sys'));
var path = require('path');
var fs = require('fs');
var lib = null;

if (fs.lstatSync(__filename).isSymbolicLink()) {
  lib = path.join(path.dirname(fs.readlinkSync(__filename)), '../lib');
} else {
  lib = path.join(__dirname, '../lib');
}

require.paths.unshift(lib);
require('cake').run();
